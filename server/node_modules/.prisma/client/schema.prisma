// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Selecoes {
  id     Int    @id @default(autoincrement())
  escudo String
  nome   String
  pts    Int
  vit    Int
  emp    Int
  der    Int
  gp     Int
  gc     Int
  sg     Int
  
  grupos Grupos[]

  @@map("selecoes")
}

model Grupos {
  id Int  @id @default(autoincrement())
  Selecoes Selecoes? @relation( fields: [fk_id], references: [id])
  fk_id Int
  
  @@map("grupos")
}
